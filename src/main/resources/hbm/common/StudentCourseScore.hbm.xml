<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="V_XG_CJXX" name="com.uws.domain.common.StudentCourseScore">
    <id name="studentId" column="XH">
      <generator class="assigned"/>
    </id>
    
     <!-- 学年 -->
    <property column="XN" name="yearCode"/>
    
     <!-- 学期 -->
    <property column="XQ" name="termCode"/>
    
     <!-- 课程 -->
    <property name="courseCode" column="KCH"/>
    
    <!-- 课程名称 -->
    <property name="courseName" column="KCMC"/>
    
    <!-- 分数 -->
    <property name="score" column="KCCJ"/>
    
  </class>
</hibernate-mapping>