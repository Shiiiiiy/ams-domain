<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="HKY_SPONSOR_INSPIRATION_AWARD" name="com.uws.domain.sponsor.InspirationalAward">
    <id name="inspirationalAwardId" column="ID" length="32">
      <generator class="uuid.hex"/>
    </id>
    <many-to-one name="studentAward" column="DIFFICULT_STUDENT_AWARD" class="com.uws.domain.sponsor.DifficultStudentAward"/>
    <many-to-one name="awardKind" column="AWARD_KIND" class="com.uws.sys.model.Dic" />
    <many-to-one name="nationalInspirationalAward" column="NATIONAL_INSPIRATIONAL_AWARD" class="com.uws.sys.model.Dic" />
    <many-to-one name="xingZhiAward" column="XING_ZHI_AWARD" class="com.uws.sys.model.Dic" />
    <many-to-one name="delStatus"  column="DEL_STATUS" class="com.uws.sys.model.Dic"/>
    <property name="createTime" column="CREATE_TIME" type="java.util.Date"/>
    <property name="updateTime" column="UPDATE_TIME" type="java.util.Date"/>
  </class>
</hibernate-mapping>
